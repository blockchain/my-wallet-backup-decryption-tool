{
  "name": "my-wallet-backupt-decryption-tool",
  "version": "1.0.2",
  "description": "Decrypt wallets backups offline",
  "main": "main.js",
  "engines": {
    "node": "4"
  },
  "scripts": {
    "start": "node_modules/.bin/electron . || node_modules/.bin/electron.cmd .",
    "postinstall": "node_modules/.bin/bower install || node_modules/.bin/bower.cmd install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node_modules/.bin/gulp build || node_modules/.bin/gulp.cmd build",
    "pack": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --all --overwrite --version=0.34.1 --icon=icons/icon",
    "pack:osx": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=darwin --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack:win32": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=win32 --arch=ia32 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack:win64": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=win32 --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack:linux32": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=linux --arch=ia32 --version=0.34.1 --icon=icons/icon",
    "pack:linux64": "node_modules/.bin/electron-packager ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=linux --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack-windows": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --all --overwrite --version=0.34.1 --icon=icons/icon",
    "pack-windows:osx": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=darwin --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack-windows:win32": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=win32 --arch=ia32 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack-windows:win64": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=win32 --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon",
    "pack-windows:linux32": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=linux --arch=ia32 --version=0.34.1 --icon=icons/icon",
    "pack-windows:linux64": "node_modules/.bin/electron-packager.cmd ./build 'My Wallet Backup Decryption Tool' --out=dist/ --platform=linux --arch=x64 --overwrite --version=0.34.1 --icon=icons/icon"
  },
  "author": "Justin Tormey <justin@blockchain.com>",
  "dependencies": {
    "blockchain-wallet-client-prebuilt": "3.22.*",
    "bower": "1.8.*",
    "jquery": "3.5.*",
    "node-env-file": "0.1.*"
  },
  "devDependencies": {
    "electron-packager": "5.1.*",
    "electron-prebuilt": "0.34.*",
    "gulp": "3.9.*",
    "gulp-clean": "0.3.*",
    "run-sequence": "1.1.*"
  }
}
